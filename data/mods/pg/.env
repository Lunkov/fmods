NAME=pg
TYPE=docker

CONTAINER_NAME=pg-test
CONTAINER_SRC=postgres:alpine

DB_NAME=test-db
DB_USER=user
DB_PASSWORD=pwd
DB_PORT=17432

CONTAINER_PORTS=17432:5432
CONTAINER_ENV_POSTGRES_PASSWORD=pwd
CONTAINER_ENV_POSTGRES_USER=user
CONTAINER_ENV_POSTGRES_DB=test-db

MIGRATE_COMMAND=--path=/migrations/ --database="postgres://user:pwd@pg-test:5432/test-db?sslmode=disable" up
MIGRATE_PATH=migrate
