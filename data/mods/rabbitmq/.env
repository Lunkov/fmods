NAME=rabbitmq
ACTIONS=run,init

CONTAINER_NAME=rabbitmq-test

CONTAINER_SRC=rabbitmq:management-alpine

CONTAINER_PORTS=5672:5672,15672:15672

RABBITMQ_URL=amqp://user:pwd@localhost:5672/11

RABBITMQ_USER=user
RABBITMQ_PASS=pwd

CONTAINER_ENV_RABBITMQ_DEFAULT_USER=user
CONTAINER_ENV_RABBITMQ_DEFAULT_PASS=pwd
CONTAINER_ENV_RABBITMQ_DEFAULT_VHOST=11

INIT_RABBITMQ_CREATE_CHANNELS=log3:fanout::log3;log1:fanout::log1
